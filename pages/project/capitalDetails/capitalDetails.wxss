/* zihexiangqing.wxss */

page {
  width: 100%;
  height: 100%;
  font-size: 11pt;
  background-color: #f7f7f7;
  margin: 0;
  padding: 0;
}

.text {
  padding: 0rpx 30rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.card {
  background-color: #fff;
  color: #888;
  padding-top: 30rpx;
  margin-bottom: 40rpx;
  position: relative;
  width: 100%;
  padding-bottom: 10rpx;
}

.Card {
  background-color: #fff;
  color: #888;
  margin-bottom: 40rpx;
}

.mubiaoLeft {
  margin: 0 0 30rpx 0;
  height: 30rpx;
}

.textDown {
  float: left;
  width: 100%;
  text-align: center;
  margin-bottom: 20rpx;
}

.textRight {
  font-size: 11pt;
  color: #353535;
  opacity: 0.6;
}

.box3 {
  height: 20px;
  background-color: #e64340;
  width: 80%;
  border-radius: 20rpx;
  background: url('data:image/png;base64,') repeat-x;
}

.box4 {
  height: 20px;
  background-color: #576b95;
  width: 80%;
  border-radius: 20rpx;
}

.boxUp {
  height: 20px;
  margin: 30rpx 30rpx;
  background-color: rgba(87, 107, 149, 0.1);
  border-radius: 20rpx;
  text-align: center;
  line-height: 20px;
  color: #353535;
}

.Icon {
  width: 140rpx;
  height: 123rpx;
  position: absolute;
  right: 1rpx;
  bottom: 1rpx;
}

.zhong {
  margin: 0 auto;
}

.cardCentre {
  margin-top: 40rpx;
  padding: 30rpx 30rpx;
  background-color: #fff;
}

.centreUp {
  flex: 1;
  width: 100%;
  font-size: 11pt;
  color: #888;
}

.centreUpLeft {
  width: 49.8%;
  padding: 30rpx 0;
  float: left;
  text-align: center;
  border-right: 1rpx solid #f7f7f7;
  border-bottom: 1rpx solid #f7f7f7;
  display: flex;
  flex-direction: column;
}

.centreDownLeft {
  width: 49.8%;
  padding: 30rpx 0;
  float: left;
  text-align: center;
  border-right: 1rpx solid #f7f7f7;
  display: flex;
  flex-direction: column;
}

.centreUpRight {
  width: 49.8%;
  padding: 30rpx 0;
  float: left;
  text-align: center;
  border-bottom: 1rpx solid #f7f7f7;
  color: #576b95;
  display: flex;
  flex-direction: column;
}

.centreDownRight {
  width: 49.8%;
  padding: 30rpx 0;
  float: left;
  text-align: center;
  color: #576b95;
  display: flex;
  flex-direction: column;
}


input {
  border: 1rpx solid #ccc;
  border-radius: 8rpx;
  padding: 16rpx 30rpx;
  width: 50%;
  margin: 30rpx auto 0;
}

.modal-input {
  width: 90%;
  height: 80rpx;
  line-height: 80rpx;
  border: 1px solid #f0f0f0;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  margin-left: 0px;
  margin-right: 0px;
  text-align: center;
  background-color: white;
}

.renchou-tip {
  width: 90%;
  height: 18px;
  font-size: 9pt;
  color: #888;
  text-align: left;
}

.biaoti {
  font-size: 11pt;
  margin: 30rpx 0 16rpx 30rpx;
}

.cardDown {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 30rpx 30rpx 30rpx 30rpx;
  border-bottom: 1rpx solid #f7f7f7;
}

.headPortrait {
  width: 140rpx;
  height: 140rpx;
  border-radius: 50%;
  margin: 0 30rpx;
}

.IconDown {
  width: 50rpx;
  height: 50rpx;
}

.cardLeftText {
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.cardLeftText text {
  margin-bottom: 30rpx;
}

.cardDownLeft {
  display: flex;
  flex-direction: row;
}

.cardRight {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  color: #576b95;
}

.left {
  position: absolute;
}

.renchou-btn {
  margin-top: 30rpx;
  width: 260rpx;
  height: 80rpx;
  line-height: 80rpx;
  color: white;
  font-size: 10pt;
}
