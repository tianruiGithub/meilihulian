/* xinxiangmu.wxss */
page{
  width: 100%;
  height: 100%;
  font-size: 11pt;
  margin: 0;
  padding: 0;
  background-color:#f7f7f7;
}
.text{
  padding-top: 60rpx;
  color: #353535;
  font-size: 13pt;
  text-align: center;
  
}
.jieshao{
  margin:30rpx;
  min-height:400rpx;
  border:1px solid #f0f0f0;
  background-color:white;
}
.jieshao-add{
  margin:0px 30rpx 30rpx 30rpx;
  height:70rpx;
}
.jieshao-add image{
  float:right;
  margin-left:20rpx;
  width:80rpx;
  height:80rpx;
}
input{
  margin: 100rpx 30rpx 0 30rpx;
  border: 1rpx solid #ccc;
  background-color: #fff;
  color: #888;
  border-radius: 10rpx;
  font-size: 11pt;
  letter-spacing: 1.2rpx;
  line-height: 90rpx;
  height: 90rpx;
}

.neirong{
  height:auto;
  border: 1rpx solid #ccc;
  background-color: #fff;
  margin: 100rpx 30rpx 0rpx 30rpx;
  border-radius: 8rpx;
  color: #888;
  text-align: center;
}
.overFlowText {
  overflow: auto;
  font-size: 11pt;
  height:300rpx;
  line-height: 180rpx;
  letter-spacing: 1.2rpx;
  margin: auto;
  margin-top:30rpx;
  margin-bottom:30rpx;
  text-align: center;
}

.center{
  overflow: auto;
  font-size: 11pt;
  height:300rpx;
  line-height: 180rpx;
  letter-spacing: 1.2rpx;
  text-align: center;
}

.overFlowTextDown {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp:5;
  overflow: hidden;
  font-size: 11pt;
  line-height: 1.4;
  letter-spacing: 1.2rpx;
  margin: 30rpx 0rpx;
}
.body-view{
  margin: 100rpx  0 0 0rpx;
  background-color: #fff;
  line-height: 90rpx;
  height: 90rpx;
  display: flex;
  justify-content: space-between;
  align-content: center;
padding:0 30rpx;
}
.big{
  transform: scale(0.8);

}
.block{
  display: block;
  width: 100%;

  

}
.none{
  display: none;
}

.btn{
  position:fixed;
  width:100%;
  bottom: 50rpx;
  font-size: 11pt;
  display: flex;
  flex-direction: row;
  justify-content: space-around;


}
.btnRight{

 height: 70rpx;
  width: 240rpx;
  background-color: #09bb07;
  border: 1rpx solid #09bb07;
  border-radius: 8rpx;
  line-height: 70rpx;
  text-align: center;
  color: #fff;
}
.btnLeft{
  height: 70rpx;
  width: 240rpx;
  background-color: #fff;
  border: 1rpx solid #09bb07;
  border-radius: 8rpx;
  line-height: 70rpx;
  text-align: center;
  color: #09bb07;
}
.matop{
  margin-top:0;

}
.Icon{
  width: 37rpx;
  height: 30rpx;
  position: absolute;
  right: 30rpx;
  top: 20rpx;
}

.map{
  margin-top: 40rpx;
  height:800rpx;
  overflow-y:auto;

}
.picture{
  margin: 30rpx auto;
  width: 100%;
  max-height: 700rpx;
}
.compile{
  position: absolute;
  top:50rpx;
  right: 30rpx;
   height: 60rpx;
  width:120rpx;
  background-color:#f7f7f7;
  border: 1rpx solid #09bb07;
  border-radius:8rpx;
  line-height: 60rpx;
  text-align: center;
  color: #09bb07;
}